{
  "name": "a-toolbox",
  "version": "1.5.1",
  "description": "javascript lightweight basic tools, isomorphic",
  "main": "index.js",
  "dependencies": {
    "hash.js": "^1.1.3"
  },
  "devDependencies": {
    "babel": "^6",
    "babel-core": "^6",
    "babel-preset-env": "^1.6",
    "babel-preset-es2015": "^6",
    "babelify": "^8",
    "browserify": "^14",
    "fs-extra": "^5",
    "gulp": "^3",
    "gulp-buffer": "^0",
    "gulp-clean": "^0.3",
    "gulp-sourcemaps": "^2.6",
    "ink-docstrap": "^1",
    "pre-commit": "^1",
    "standard": "^11",
    "tap": "^11",
    "tollo": "0.0.3-dev",
    "vinyl-source-stream": "^1"
  },
  "pre-commit": [
    "test",
    "build"
  ],
  "scripts": {
    "test": "standard && tap test/tdd.js",
    "test-cov": "standard && tap test/tdd.js --cov",
    "build": "gulp",
    "clear": "gulp clear",
    "_doxdox": "mkdir -p doc/doxdox; doxdox './src/*.js' --layout markdown --output ./doc/README.md",
    "_jsdoc": "jsdoc -c doc/jsdoc.json -t ./node_modules/ink-docstrap/template -R README.md src/*.js"
  },
  "standard": {
    "ignore": [
      "test/",
      "_test/",
      "src/_*"
    ]
  },
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/braceslab/a-toolbox.git"
  },
  "author": "Simone Sanfratello <simone@braceslab.com>",
  "license": "MIT",
  "keywords": [
    "library",
    "toolkit",
    "toolbox",
    "array",
    "fs",
    "object",
    "hash",
    "task",
    "async",
    "random",
    "string"
  ]
}
