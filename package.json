{
  "name": "a-toolbox",
  "version": "1.6.1",
  "description": "javascript lightweight basic tools for node.js and browser",
  "main": "index.js",
  "dependencies": {
    "hash.js": "^1.1.5",
    "tollo": "0.0.7-dev"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "fs-extra": "^7.0.0",
    "gulp": "^3.9.1",
    "gulp-buffer": "^0",
    "gulp-clean": "^0.3",
    "gulp-sourcemaps": "^2.6",
    "ink-docstrap": "^1",
    "pre-commit": "^1",
    "standard": "^11.0.1",
    "tap": "^12.0.1",
    "vinyl-source-stream": "^1"
  },
  "pre-commit": [
    "test",
    "build"
  ],
  "scripts": {
    "test": "standard && tap test/tdd.js",
